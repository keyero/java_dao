package day04;

import java.util.Scanner;

/**
 * 找出三个数中的最大值
 */
public class Demo06 {

    public static void main(String[] args) {
        int a, b, c;
        Scanner sc = new Scanner(System.in);
        System.out.print("输入三个数:");
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        int max;
        if (a>b)
            max = a;
        else
            max = b;
        if (c>max)
            max = c;
        System.out.println("最大值:"+max);
        
        
    }

}
