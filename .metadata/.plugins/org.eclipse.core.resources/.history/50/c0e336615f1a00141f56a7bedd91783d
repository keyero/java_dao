package day02;

import java.util.Scanner;

public class Demo05 {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("输入距离:");
        double t,s,g;
        g = 9.8;
        s = sc.nextDouble();
        t = Math.sqrt((2*s)/g);
        System.out.println("时间(秒):"+t);
        
    }

}
