package day06;

import java.util.Arrays;

/**
 * 数组的复制
 */
public class Demo07 {

    public static void main(String[] args) {
        int[] ary1 = {5,6,7};
        int[] ary2 = ary1;
        ary1[0] = 10;
        System.out.println(Arrays.toString(ary1));
        System.out.println(Arrays.toString(ary2));
        
        
        
    }

}
