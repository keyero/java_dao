package day04;

import java.util.Scanner;

/**
 * 计算三个数中的最大数
 *
 */
public class Demo04 {

    public static void main(String[] args) {
        int a, b, c;
        Scanner sc = new Scanner(System.in);
        System.out.print("输入三个数(a b c)":);
        a = sc.nextInt();
        b = sc.nextInt();
        c = sc.nextInt();
        int max;
        max = a>b ? a : b;
        max = max>c ? max : c;
        System.out.println(max);
    }

}
